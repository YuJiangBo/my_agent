<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yu.my_agent.mapper.mysql.UserMapper">

    <select id="selectUsersByEmailContaining" parameterType="string" resultType="com.yu.my_agent.entity.mysql.User">
        SELECT * FROM users WHERE email LIKE CONCAT('%', #{email}, '%')
    </select>

</mapper>
